{
    "version": 2,
    "builds": [
        {
            "src": "build/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.js"
        }
    ],
    "build": {
        "env": {
            "PRISMA_CLIENT_ENGINE_TYPE": "binary"
        }
    },
    "regions": [
        "sfo1"
    ] 
}